
select count(*) as apps,worksite,year from h1b_final where case_status = 'CERTIFIED' and year = '2011' group by worksite,year order by apps DESC LIMIT 5;

select count(*) as apps,worksite,year from h1b_final where case_status = 'CERTIFIED' and year = '2012' group by worksite,year order by apps DESC LIMIT 5;

select count(*) as apps,worksite,year from h1b_final where case_status = 'CERTIFIED' and year = '2013' group by worksite,year order by apps DESC LIMIT 5;

select count(*) as apps,worksite,year from h1b_final where case_status = 'CERTIFIED' and year = '2014' group by worksite,year order by apps DESC LIMIT 5;

select count(*) as apps,worksite,year from h1b_final where case_status = 'CERTIFIED' and year = '2015' group by worksite,year order by apps DESC LIMIT 5;

select count(*) as apps,worksite,year from h1b_final where case_status = 'CERTIFIED' and year = '2016' group by worksite,year order by apps DESC LIMIT 5;
