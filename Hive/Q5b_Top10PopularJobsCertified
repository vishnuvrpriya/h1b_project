select count(*) as apps,year,job_title from h1b_final where case_status = 'CERTIFIED' and year = '2014' group by year,job_title order by apps desc limit 10;

select count(*) as apps,year,job_title from h1b_final where case_status = 'CERTIFIED' and year = '2015' group by year,job_title order by apps desc limit 10;

select count(*) as apps,year,job_title from h1b_final where case_status = 'CERTIFIED' and year = '2016' group by year,job_title order by apps desc limit 10;
